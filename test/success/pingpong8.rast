type ping  = +{ping : &{pong : ping}}

proc asset pong : (x : ping) |- (sink : 1) =
  case x
  ( ping => x.pong; sink <- pong <- x
  )

proc asset ping : (sink : 1) |- (x : ping) =
  x.ping;
  case x
  ( pong => x <- ping <- sink
  )

proc asset pingpong : (sink : 1) |- (x : ping) =
  x.ping;
  case x
  ( pong => x <- ping <- sink
  )